# /etc/network/interfaces -- configuration file for ifup(8), ifdown(8)

# The loopback interface
# automatically added when upgrading
auto lo
iface lo inet loopback

# The first network card - this entry was created during the Debian installation
# automatically added when upgrading

##    KEY="87c3f86fd5bbdf56b26157f370" # ASAHI Net IH

auto eth1
iface eth1 inet dhcp
	wireless-essid gree-wireless-sr
#	wireless-key restricted kogai
	wireless-key1 s:gree_ost
	wireless-key2 s:gree_ost
	wireless-key3 s:gree_ost
	wireless-key4 s:gree_ost
	wireless-ap 00:16:01:04:73:FB
#	wireless-channel 8
	wireless-mode Auto
